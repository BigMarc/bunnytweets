# Browser provider: "gologin" (default) or "dolphin_anty"
BROWSER_PROVIDER=gologin

# GoLogin API Token (optional, can also be set in settings.yaml)
GOLOGIN_TOKEN=
# GOLOGIN_HOST=localhost
# GOLOGIN_PORT=36912

# Dolphin Anty API Token (optional, can also be set in settings.yaml)
DOLPHIN_ANTY_TOKEN=
# DOLPHIN_ANTY_HOST=localhost
# DOLPHIN_ANTY_PORT=3001

# Google Drive credentials file path override
GOOGLE_CREDENTIALS_FILE=config/credentials/google_credentials.json

# Timezone
TZ=America/New_York
